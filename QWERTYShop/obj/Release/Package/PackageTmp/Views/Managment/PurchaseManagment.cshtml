@model QWERTYShop.Models.PurchaseManagmentModels
@{
    ViewBag.Title = "PurchaseManagment";
}

<h2>Управление заказами</h2>

@foreach (var element in ViewBag.Data)
{
    <p>@element.ToString()</p>
    <form method="post">
        <input type="submit" value="@element.Condition" name="Condition"/>
        <input type="number" hidden name="Id" value="@element.Id" name="Id"/>
    </form>
   
}

